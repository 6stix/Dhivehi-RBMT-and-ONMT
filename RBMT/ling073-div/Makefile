	  echo " $(MKDIR_P) '$(DESTDIR)$(apertium_divdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(apertium_divdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
