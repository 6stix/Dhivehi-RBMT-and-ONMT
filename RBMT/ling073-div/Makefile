	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(apertium_divdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(apertium_divdir)" || exit $$?; \
	done

